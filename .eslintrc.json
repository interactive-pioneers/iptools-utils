{
  "parserOptions": {
    "ecmaVersion": 2017
  },
  "rules": {
    "curly": [
      2,
      "all"
    ],
    "operator-linebreak": [
      2,
      "after"
    ],
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "max-len": [
      2,
      120
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "no-multi-str": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "space-unary-ops": [
      2,
      {
        "nonwords": false,
        "overrides": {}
      }
    ],
    "one-var": [
      2,
      "never"
    ],
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "keyword-spacing": [
      2,
      {}
    ],
    "space-infix-ops": 2,
    "space-before-blocks": [
      2,
      "always"
    ],
    "eol-last": 2,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "no-multiple-empty-lines": 2,
    "no-alert": 2,
    "no-eval": 2,
    "no-unreachable": 2,
    "eqeqeq": 2,
    "radix": 2
  }
}
