{
  "ignorePatterns": [
    "package-scripts.js",
    "scripts.js",
    "**/scripts/*.*",
    "**/*.config.js",
    "**/node_modules/**/*",
    "**/config/*"
  ],
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 13,
    "sourceType": "module"
  },
  "rules": {
    "curly": [
      2,
      "all"
    ],
    "operator-linebreak": [
      2,
      "after"
    ],
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ],
    "max-len": [
      2,
      120
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "no-multi-str": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "space-unary-ops": [
      2,
      {
        "nonwords": false,
        "overrides": {}
      }
    ],
    "one-var": [
      2,
      "never"
    ],
    "brace-style": [
      2,
      "1tbs",
      {
        "allowSingleLine": true
      }
    ],
    "keyword-spacing": [
      2,
      {}
    ],
    "space-infix-ops": 2,
    "space-before-blocks": [
      2,
      "always"
    ],
    "eol-last": 2,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "array-bracket-spacing": [
      2,
      "never"
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "no-const-assign": "error",
    "prefer-const": "error",
    "no-multiple-empty-lines": 2,
    "no-bitwise": 2,
    "no-console": "warn",
    "no-alert": "error",
    "no-eval": 2,
    "no-unreachable": 2,
    "no-use-before-define": 2,
    "new-cap": "warn",
    "no-caller": "error",
    "require-unicode-regexp": 2,
    "no-unused-vars": "warn",
    "no-undef": "error",
    "no-shadow": "error",
    "no-constant-condition": "error",
    "eqeqeq": 2,
    "radix": 2,
    "no-dupe-keys": "error",
    "use-isnan": "error"
  }
}
